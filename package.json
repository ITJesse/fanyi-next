{
  "name": "fanyi-tool-next",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cli": "ts-node -r dotenv/config -r tsconfig-paths/register -O '{\"module\": \"commonjs\", \"target\": \"es2018\"}' scripts/index.ts"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.12",
    "@devexperts/remote-data-ts": "^2.1.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "dotenv": "^10.0.0",
    "focus-visible": "^5.2.0",
    "fp-ts": "^2.11.5",
    "framer-motion": "^4",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "next": "12.0.3",
    "next-seo": "^4.28.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "@types/react": "17.0.34",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "4.4.4"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "never"
  },
  "eslintConfig": {
    "extends": [
      "next",
      "next/core-web-vitals",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "react/jsx-key": 0,
      "react/display-name": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-unused-vars": [
        1,
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "@typescript-eslint/no-var-requires": 0
    }
  }
}
